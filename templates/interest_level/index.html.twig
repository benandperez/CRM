{% extends 'base.html.twig' %}

{% block title %}InterestLevel index{% endblock %}

{% block body %}
    <h1>InterestLevel index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Status</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for interest_level in interest_levels %}
            <tr>
                <td>{{ interest_level.id }}</td>
                <td>{{ interest_level.name }}</td>
                <td>{{ interest_level.status ? 'Yes' : 'No' }}</td>
                <td>
                    <a href="{{ path('app_interest_level_show', {'id': interest_level.id}) }}">show</a>
                    <a href="{{ path('app_interest_level_edit', {'id': interest_level.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_interest_level_new') }}">Create new</a>
{% endblock %}
