{% extends 'base.html.twig' %}

{% block title %}PropertyClass{% endblock %}

{% block body %}
    <h1>PropertyClass</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ property_class.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ property_class.name }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ property_class.status ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_property_class_index') }}">back to list</a>

    <a href="{{ path('app_property_class_edit', {'id': property_class.id}) }}">edit</a>

    {{ include('property_class/_delete_form.html.twig') }}
{% endblock %}
