{% extends 'base.html.twig' %}

{% block title %}Project{% endblock %}

{% block body %}
    <h1>Project</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ project.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ project.name }}</td>
            </tr>
            <tr>
                <th>ShortName</th>
                <td>{{ project.shortName }}</td>
            </tr>
            <tr>
                <th>SeparatesWith</th>
                <td>{{ project.separatesWith }}</td>
            </tr>
            <tr>
                <th>Ubication</th>
                <td>{{ project.ubication }}</td>
            </tr>
            <tr>
                <th>ProjectFinishes</th>
                <td>{{ project.projectFinishes }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_project_index') }}">back to list</a>

    <a href="{{ path('app_project_edit', {'id': project.id}) }}">edit</a>

    {{ include('project/_delete_form.html.twig') }}
{% endblock %}
